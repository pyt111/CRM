<script>
import axios from "axios";
export default {
    data() {
        return {
            msg: "123",
            result: "",
             showText: true,
             showText2: true,
            // 提成提现
            allUserBalance: "",
            modal: false,
            modals: {
                one: "",
                bankName: "10,000",
                bankNumber: "",
                everyPrice: "",
                img: "",
                poundage: "",
                userBalance: ""
            }
        };
    },
    methods: {
        tichengAxios() {
            // console.log(this.userId)
            let data = {
                userId: this.userId,
                type: 1
            };
            this.loading = true;
            this.post(
                process.env.BASE_API + "/agent/commWithdrawalConditions",
                data
            )
                .then(reponse => {
                    this.loading = false;
                    let res = reponse.result.dataList;
                    this.modals = res;
                    this.modals.bankNumber = res.bankNumber.slice(-4);
                    // console.log(reponse.result.dataList)
                    // this.$message.info(reponse.data.message)
                })
                .catch(err => {
                    console.log(err);
                    this.$Message.error(err.message);
                });
        },
        tixian() {
            this.modal = true;
            this.tichengAxios();
        },
        modalsAll() {
            // this.allUserBalance = this.modals.userBalance;
            if (this.modals.userBalance <= this.modals.everyPrice) {
                this.allUserBalance = this.modals.userBalance;
            }else if(this.modals.userBalance > this.modals.everyPrice){
                this.allUserBalance = this.modals.everyPrice * 10000;
            }
            //  console.log(this.modals.one)
        },
        // 提成提现确认接口
        tichengAxiosOk() {
            // console.log(this.userId)
            let data = {
                money: this.allUserBalance,
                userId: this.userId,
                isAgentId: this.userId
            };
            // this.loading = true;
            if (
                this.allUserBalance * 1 <= this.modals.poundage * 1 ||
                this.allUserBalance * 1 > this.modals.everyPrice * 10000 
            ) {
                this.showText = false;
                this.showText2 = true;
            } else if( this.allUserBalance * 1 > this.modals.userBalance) {
                 this.showText2 = false;
                 this.showText = true;
            }else if (
                this.allUserBalance * 1 > this.modals.poundage * 1 &&
                this.allUserBalance * 1 <= this.modals.everyPrice * 10000
            ) {
                 this.showText2 = true;
                 this.showText = true;
                this.post(process.env.BASE_API + "/agent/commWithdrawal", data)
                    .then(reponse => {
                        this.loading = false;

                        console.log(reponse.code);
                        if (reponse.code == 0) {
                            this.$Message.success("提现成功");
                            setTimeout(() => {
                                this.modal = false;
                                this.allUserBalance = "";
                            }, 1000);
                        } else {
                            //this.$Message.success(reponse.message)
                        }
                    })
                    .catch(err => {
                        console.log(err);
                        this.$Message.error(err.message);
                    });
            }
        },
        modalsAll() {
            this.allUserBalance = this.modals.userBalance;
            //  console.log(this.modals.one)
        },
        modalsOk() {
            this.tichengAxiosOk();
        },
        modalsNo() {
            this.modal = false;
            this.allUserBalance = "";
        }
    },
    mounted() {
        let params = {
            userName: this.$store.getters.userName
        };
        this.get(process.env.BASE_API + "/agent/panel", params)
            .then(reponse => {
                var res = reponse.result;
                this.result = res;
                // console.log(res)
            })
            .catch(error => {
                //alert('网络错误')
            });
    },
    created() {
        this.userId = this.$store.getters.uid;
    }
};
</script>
